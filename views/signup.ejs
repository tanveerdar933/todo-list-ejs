<%-include("htmlHeader")-%>
<div class="container">
  <div class="row justify-content-center">
    <div class="col-5">
      <div class="heading">
        <h2>Sign Up Form</h2>
      </div>
      <form class="mt-5 text-center" action="" method="post">
        <div class="form-group mt-2">
          <label for="firstName" class="form-label visually-hidden">First Name</label>
          <input class="form-control" type="text" id="firstName" placeholder="First Name">
          <small class="form-text text-muted text-left visually-hidden">required</small>
        </div>
        <div class="form-group mt-2">
          <label for="lastName" class="form-label visually-hidden">Last Name</label>
          <input class="form-control" type="text" id="lirstName" placeholder="Lirst Name">
          <small class="form-text text-muted text-left visually-hidden">required</small>
        </div>
        <div class="form-group mt-2">
          <label for="email" class="form-label visually-hidden">Email</label>
          <input class="form-control" type="email" id="email" placeholder="fake@example.com">
          <small class="form-text text-muted text-left visually-hidden">required</small>
        </div>
        <div class="form-group mt-2">
          <label for="password" class="form-label visually-hidden">Password</label>
          <input class="form-control" type="password" id="password" placeholder="Password">
          <small class="form-text text-muted text-left visually-hidden">required</small>
        </div>
        <button class="btn btn-success w-25 mt-3" type="submit">Sign Up</button>
      </form>
    </div>
  </div>
</div>
<%-include("htmlFooter")-%>